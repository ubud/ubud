{
    "name": "ubud",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "publish:ubud": "run-p publish:ubud:*",
        "build:ubud": "run-s build:ubud:step:*",
        "build:ubud:step:1": "run-p build:ubud:bootstrap-theme build:ubud:dropdown build:ubud:form build:ubud:http build:ubud:loader build:ubud:menus build:ubud:ngrx build:ubud:storage build:ubud:template build:ubud:utilities build:ubud:sate",
        "build:ubud:step:2": "run-p build:ubud:tooltip build:ubud:user-control",
        "build:ubud:bootstrap-theme": "ncp ./src/ubud/bootstrap-theme ./dist/ubud/bootstrap-theme",
        "publish:ubud:bootstrap-theme": "npm publish dist/ubud/bootstrap-theme --access public",
        "build:ubud:dropdown": "ng build @ubud/dropdown --prod",
        "publish:ubud:dropdown": "npm publish dist/ubud/dropdown --access public",
        "build:ubud:form": "ng build @ubud/form --prod",
        "publish:ubud:form": "npm publish dist/ubud/form --access public",
        "build:ubud:http": "ng build @ubud/http --prod",
        "publish:ubud:http": "npm publish dist/ubud/http --access public",
        "build:ubud:loader": "ng build @ubud/loader --prod",
        "publish:ubud:loader": "npm publish dist/ubud/loader --access public",
        "build:ubud:menus": "ng build @ubud/menus --prod",
        "publish:ubud:menus": "npm publish dist/ubud/menus --access public",
        "build:ubud:ngrx": "ng build @ubud/ngrx --prod",
        "publish:ubud:ngrx": "npm publish dist/ubud/ngrx --access public",
        "build:ubud:storage": "ng build @ubud/storage --prod",
        "publish:ubud:storage": "npm publish dist/ubud/storage --access public",
        "build:ubud:template": "ng build @ubud/template --prod",
        "publish:ubud:template": "npm publish dist/ubud/template --access public",
        "build:ubud:tooltip": "ng build @ubud/tooltip --prod",
        "publish:ubud:tooltip": "npm publish dist/ubud/tooltip --access public",
        "build:ubud:user-control": "ng build @ubud/user-control --prod",
        "publish:ubud:user-control": "npm publish dist/ubud/user-control --access public",
        "build:ubud:utilities": "ng build @ubud/utilities --prod",
        "publish:ubud:utilities": "npm publish dist/ubud/utilities --access public",
        "build:ubud:sate": "ng build @ubud/sate --prod",
        "publish:ubud:sate": "npm publish dist/ubud/sate --access public",
        "prettier": "prettier --config ./.prettierrc --write \"{src,apps}/**/*{.js,.ts,.json,.scss}\""
    },
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "@angular/animations": "~9.1.9",
        "@angular/cdk": "^9.2.4",
        "@angular/common": "~9.1.9",
        "@angular/compiler": "~9.1.9",
        "@angular/core": "~9.1.9",
        "@angular/forms": "~9.1.9",
        "@angular/platform-browser": "~9.1.9",
        "@angular/platform-browser-dynamic": "~9.1.9",
        "@angular/router": "~9.1.9",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "@ionic/storage": "^2.2.0",
        "@ngrx/effects": "^9.2.0",
        "@ngrx/router-store": "^9.2.0",
        "@ngrx/store": "^9.2.0",
        "@ngrx/store-devtools": "^9.2.0",
        "bootstrap": "^4.5.0",
        "hamburgers": "^1.1.3",
        "ng-sidebar": "^9.3.0",
        "ngx-cookie-service": "^3.0.4",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~15.0.4",
        "@angular-devkit/build-ng-packagr": "~0.901.7",
        "@angular/cli": "~9.1.7",
        "@angular/compiler-cli": "~9.1.9",
        "@angular/language-service": "^9.1.9",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "husky": "^4.2.5",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "ncp": "^2.0.0",
        "ng-packagr": "^9.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.8.3"
    }
}
